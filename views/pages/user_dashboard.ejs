<!doctype html>
<html lang="en">

<head>
     <meta charset="UTF-8">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>User Dashboard</title>
     <!-- Bootstrap CSS Link -->
     <!-- <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> -->
     <script src="//code.jquery.com/jquery-1.11.1.min.js"></script> 
     <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
     <link rel="stylesheet" href="css/user_dashboard.css">
     <link rel="stylesheet" href="css/formValidation.css">
     <script src="/js/driver_registration_form.js"></script>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
     <script src="/js/state_city.js"></script>
</head>
<body>

     <!-- adding navbar -->
    <%- include('../partials/navbar') %>
    <!-- Adding driver registration Form -->
    <%- include('../partials/driver_registration_form'); %>
    <%- include('../partials/reserved_driver_registration_form') %>
    <%- include('../partials/vehicle_registration_form') %>
    <%- include('../partials/user_registration_form') %>
    <%- include('../partials/vehicle_insurance_form') %>
    <%- include('../partials/delete_form_modal') %>

     <div class="container-fluid">
          <div class="row">
               <!-- Main Menu -->
               <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" style="background: #5359E2;"> 
                    <div id="accordian" class="menu-parent style1">
                        <ul class="menu">
                            <li class="menu-child">
                                <div class="text-center">
                                    <i class="fas fa-user-circle text-light display-2 m-4"></i><br>
                                    <% if(user.name){ %>
                                        <span class="text-light fw-bold"><%= user.name %></span><br>
                                        <span class="text-light fw-bold"><%= user.contact_no %></span>
                                    <% }else{ %>
                                        <span class="text-light fw-bold"><%= user.email %></span><br>
                                        <span class="text-light fw-bold"><%= user.contact_no %></span>
                                    <% } %>
                                </div>
                            </li>
                            <li class="menu-child">
                                <a class="dashboard-links" onclick="dashboardChoices('my-profile')" ><i class="fas fa-user text-light fs-6"></i> &nbsp;My Profile</a>
                              </li>
                          <li class="menu-child">
                            <a href="#"><i class="fa fa-tachometer"></i> User Dashboard</a>
                            <ul>
                              <li> <a class="dashboard-links" id="user-booking-history" onclick="dashboardChoices('my-bookings')" >My Current Bookings</a></li>
                              <li> <a class="dashboard-links" id="user-booking-history" onclick="dashboardChoices('my-bookings')" >My Booking History</a></li>
                            </ul>
                          </li>
                          <% if(user.is_owner){ %>
                          <li class="menu-child">
                            <a href="#"><i class="fa fa-tachometer"></i> Owner Dashboard</a>
                            <ul>
                                <li><a class="dashboard-links" onclick="dashboardChoices('my-vehicles')">My Vehicles</a></li>
                                <li> <a class="dashboard-links" onclick="dashboardChoices('my-drivers')">My Drivers</a></li>
                                <li> <a class="dashboard-links" onclick="dashboardChoices('my-current-bookings')" id="current-bookings">My Current Bookings</a></li>
                                <li> <a class="dashboard-links" onclick="dashboardChoices('my-vehicle-booking-history')">My Vehicle Bookings History</a></li>
                                <li> <a class="dashboard-links" onclick="dashboardChoices('my-booking-request')" id="vehicles-bookings-request">My Booking Requests</a></li>
                            </ul>
                          </li>
                          <% }%>
                    
                          <li class="menu-child">
                            <a class="dashboard-links" onclick="dashboardChoices('my-wallet')" id="wallet-dashboard"><i class="fa-solid fa-wallet" style="color: #ffffff;"></i>&nbsp; My Wallet</a>
                          </li>
                          <li class="menu-child">
                            <% if(!user.password){%>
                                <a class="dashboard-links" onclick="dashboardChoices('change-password')" ><i class="fa-solid fa-key" style="color: #ffffff;"></i>&nbsp; Create Password</a>
                            <%}else{%>
                                <a class="dashboard-links" onclick="dashboardChoices('change-password')" ><i class="fa-solid fa-key" style="color: #ffffff;"></i>&nbsp; Change Password</a>
                             <%}%>
                          </li>

                        <% if(!user.is_owner && user.is_complete){%>  
                            <li class="">
                                <a  class="dashboard-links" href="/user/registerowner">Become Owner</a> 
                            </li>
                        <%}else if(!user.is_owner && !user.is_complete){%>
                            <li class="menu-child">
                                <a class="dashboard-links" id="book-vehicle-page-btn" data-bs-toggle="modal" data-bs-target="#userRegistrationForm">Become Owner</a>
                            </li>
                        <%}%>

                            <li class="">
                                <a href="/user/logoutuser"><i class="fa fa-sign-out"></i> Logout</a>
                            </li>
                        </ul>
                      </div>
                </div>
          <!-- Main Menu -->

           <!-- User Details -->
            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 " id="my-profile" >
                <div class="d-sm-flex m-2"  style="background-color: white; width: 98%; ">
                    <div class="offset-lg-4 offset-md-4 col-lg-8 col-md-8 offset-sm-3 col-sm-9 col-xs-12" >
                        <img src="images/profilePic.jpeg"  width="25%" alt="" style="border-radius: 50%;" class="mt-lg-4" ><br>
                        <i class="fa-solid fa-pen-to-square" style="margin-left: 12%; margin-top: 0;" onclick="uploadProfileImage()"></i><br>
                        <%if(user.name){%>
                            <label for="name" class="fw-bold fs-5 my-1">Name : </label><span class="fs-6 fw-bold ms-2"><%= user.name %></span><br>
                        <%}%>
                        <%if(user.gender){%>
                            <label for="gender" class="fw-bold fs-5 my-1">Gender : </label><span class="fs-6 fw-bold ms-2"><%= user.gender %></span><br>
                        <%}%>
                        <%if(user.email){%>
                            <label for="name" class="fw-bold fs-5 my-1">Email : </label><span class="fs-6 fw-bold ms-2"><%= user.email %></span><br>
                        <%}%>
                        <%if(user.contact_no){%>
                            <label for="name" class="fw-bold fs-5 my-1">Contact : </label><span class="fs-6 fw-bold ms-2"><%= user.contact_no%></span><br>
                        <%}%>
                        <%if(user.address){%>
                            <label for="name" class="fw-bold fs-5 my-1">Address : </label><span class="fs-6 fw-bold ms-2"><%= user.address %></span><br>
                        <%}%>
                        <%if(user.city){%>
                            <label for="name" class="fw-bold fs-5 my-1">City : </label><span class="fs-6 fw-bold ms-2"><%= user.city %></span><br>
                        <%}%>
                        <%if(user.state){%>
                            <label for="name" class="fw-bold fs-5 my-1">State : </label><span class="fs-6 fw-bold ms-2"><%= user.state %></span><br>
                        <%}%>
                        <%if(user.pin_code){%>
                            <label for="name" class="fw-bold fs-5 my-1">Pincode : </label><span class="fs-6 fw-bold ms-2"><%= user.pin_code %></span><br>
                        <%}%>
                        <%if(user.aadhar_number){%>
                            <label for="name" class="fw-bold fs-5 my-1">Aadhar Number : </label><span class="fs-6 fw-bold ms-2"><%= user.aadhar_number %></span><br>
                        <%}%>
                        <%if(user.pan_number){%>
                            <label for="name" class="fw-bold fs-5 my-1">Pan Number : </label><span class="fs-6 fw-bold ms-2"><%= user.pan_number %></span><br>
                        <%}%>
                        <% if(user.is_complete){%>
                            <!-- <button class="btn btn-primary btn-md my-5 w-50 " style="background-color: #5647F9;">Update Details</button> -->
                            <button class="open-modal-user btn btn-primary btn-md my-5 w-50 " style="background-color: #5647F9;" data-bs-toggle="modal" data-bs-target="#userDetailsUpdateForm" data-user_details="<%=user._id%>">Update Details</button>
                        <% } else{%>
                            <button class="btn btn-primary button-shadow w-50 ms-2 mt-5" id="book-vehicle-page-btn" data-bs-toggle="modal" data-bs-target="#userRegistrationForm">Complete Profile</button>
                        <%}%>
                    </div>
                </div>
            </div>
          <!-- User Details -->
            <!-- User Update Modal Start -->
            <!-- Modal for Update Profile Details Start-Changes are running  -->
          <div class="modal fade" id="userDetailsUpdateForm" tabindex="-1" data-bs-backdrop="static" data-bs-keyboard="false" role="dialog" aria-labelledby="modalTitleId" aria-hidden="true">
            <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl " role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="modalTitleId">User Details Update Form </h5>
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
          
                <div class="modal-body">
                  <form   class="form" enctype="multipart/form-data">
                  <div class="container ">
                    <div class="border bg-primary">
                      <h5 class="m-2 fw-bold text-center text-light">USER PROFILE UPDATE FORM</h5>
                    </div>
                  </div>
                  <div class="container"><br>
                    <div class="row">
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-name" class=" form-label fw-bold">Name</label>
                        <input type="text" class="form-control w-75" placeholder="Enter Full Name" name="name" id="username" onkeyup="checkUsername3()" autocomplete="off">
                        <small></small>
                      </div>
                      <div class="mb-3 col-lg-6"><br>
                          <label for="gender" class=" form-label fw-bold ">Gender</label>
                          <input type="radio" class="border-dark fw-bold mx-3" value="Male" checked name="gender" id="male" aria-describedby="ride"><label for="male" class="fw-bold">Male</label>
                          <input type="radio" class="border-dark fw-bold mx-3" value="Female" name="gender" id="female" aria-describedby="ride"><label for="female" class="fw-bold">Female</label>
                          <input type="radio" class="border-dark fw-bold mx-3" value="Other" name="gender" id="other" aria-describedby="ride"><label for="other" class="fw-bold">Other</label>
                        </div>
                      </div>
                    <div class=" row ">
                      
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-address" class="form-label fw-bold ">Address</label>
                        <input type="text" class="form-control  w-75 " placeholder="Enter Address" id="useraddress" name="address" aria-describedby="address" onkeyup="checkAddress3()">
                        <small></small>
                      </div>
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="pincode" class=" form-label fw-bold ">Pincode</label>
                        <input type="text" class="form-control w-75 " placeholder="Enter Pincode" id="userpincode" name="pincode" aria-describedby="ride" onkeyup="checkPincode3()">
                        <small></small>
                      </div>
                    </div>
          
                    <div class=" row">
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-state" class=" form-label fw-bold ">State</label>
                        <select onchange="print_city('userupdatecity', this.selectedIndex);" id="userupdatestate" name ="state" class="form-control  w-75"></select>
                        <small></small>
                      </div>
          
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-city" class=" form-label fw-bold ">City</label>
                        <select id ="userupdatecity" class="form-control w-75" name="city" ></select>
                        <script language="javascript">print_state("userupdatestate");</script>
                        <small></small>
                      </div>
                    </div>
          
                    <div class=" row ">
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-aadhar-number" class=" form-label fw-bold ">Aadhar Number</label>
                        <input type="text" class="form-control  w-75 " placeholder="Enter Aadhar Number" id="useraadharnumber" name="aadharnumber" aria-describedby="ride" onkeyup="checkAadharNumber3()">
                        <small></small>
                      </div>
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-aadhar-img" class=" form-label fw-bold">Upload Aadhar Image</label>
                        <input type="file" class="form-control w-75" placeholder="Upload Aadhar image here" id="useraadharimg" name="aadharimg" aria-describedby="ride" accept="image/*" onchange="checkAadharImage3()">
                        <small></small>
                      </div>
                    </div>
                    <div class="row">
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-pan-number" class=" form-label fw-bold ">Pan Card Number (optional)</label>
                        <input type="text" class="form-control w-75 " placeholder="Enter Pancard Number" name="pannumber" id="userpannumber" onkeyup="checkPanCardNumber3()" aria-describedby="ride">
                        <small></small>
                      </div>
                      <div class="mb-3 col-lg-6 form-field">
                        <label for="user-pancard-image" class=" form-label fw-bold">Upload Pan Card Image (optional)</label>
                        <input type="file" class="form-control w-75" placeholder="UploadPan Card image here" name="panimg" id="userpancardimage" accept="image/*" onchange="checkPanCardImage3()" aria-describedby="ride">
                        <small></small>
                      </div>
                    </div>
                  </div>
          
                  <div class="row mt-2">
                    <div class="mb-3 col-lg-6 form-field">
                      <input type="button" class="btn btn-secondary w-75 ms-2" value="cancel" data-bs-dismiss="modal">
                    </div>
                    <div class="mb-3 col-lg-6 form-field">
                      <input type="submit" class="btn btn-primary w-75" value="Submit">
                    </div>
                  </div>
                </form>
              </div>
              </div>
            </div>
          </div>
          
          <!-- Modal for Update Profile Details End -->
            <!-- User Update Modal End  -->

           <!-- Booking History -->
            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12" id="my-bookings">
                <h4 class="text-center fw-bold m-2 " style="background-color: white;">View Booking History</h4>
                <div class="table-responsive" id="user-booking"></div>
            </div>
          <!-- Booking History -->

        <!-- User Create Password Start -->
        <%if(!user.password ){%>
        <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12" id="change-password">
            <h4 class="text-center fw-bold my-2 m-2 " style="background-color: white; width: 98%;">Create Password</h4>
            <div align="center" style="background-color: white;">
                <form action="/user/createpassword" method="post" onsubmit="return submitCreateFormData()">
                    <div class="my-5 form-field">
                        <label class="fw-bold fs-5 " for="newpassword">Enter Password*</label>
                        <input type="password" class="w-75 form-control" placeholder="Enter password" id="createnewpass" name="newpassword" onkeyup="createCheckNewPass4()">
                        <small></small>
                    </div>
                    <div class="my-5 form-field">
                        <label class="fw-bold fs-5" for="createconfirmpass">Confirm Password*</label>
                        <input type="password" class="w-75 form-control" placeholder="Enter confirm password" id="createconfirmpass" name="createconfirmpass" onkeyup="createCheckCnfPass4()">
                        <small></small>
                    </div>
                    <div class="mt-5">
                        <input type="submit" class="btn btn-primary w-75 fs-5 fw-bold form-control" value="Create">
                    </div><br><br>
                </form>
            </div>
        </div>    
        <!-- User Create Password End -->
        <%}else{%>
        <!-- User Change Password  Start-->
        <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12" id="change-password" >
            <div class="row  ms-lg-1 mt-4">
                <h4 class="text-center fw-bold my-2 m-2 ">Change Password</h4>
                <div class="m-2" style="background-color: white; width: 98%;"> 
                    <form action="/user/changepassword" onsubmit=" return submitchangepassword()" method="post">
                        <div align="center" style="background-color: white;">
                            <div class="form-field">
                                <label class=" fw-bold fs-5 mt-3" for="changeoldpassword">Old Password</label>
                                <input type="password" class="w-50 form-control mt-2" placeholder="Enter Old Password" id="changeoldpassword" name="changeoldpassword" onkeyup="checkChangeOldPass4()">
                                <small></small>
                            </div>
                            <div class="mt-4 form-field">
                                <label class="fw-bold fs-5 " for="changenewpassword">New Password</label>
                                <input type="password" class="w-50 form-control mt-2" placeholder="Enter Old Password" id="changenewpassword" name="changenewpassword" onkeyup="checkChangeNewPass4()">
                                <small></small>
                            </div>
                            <div class="mt-4 form-field">
                                <label class="fw-bold fs-5  " for="changecnfpassword">Confirm Password</label>
                                <input type="password" class="w-50 form-control mt-2" placeholder="Enter Old Password" id="changecnfpassword" name="changecnfpassword" onkeyup="checkChangeCnfPass4()">
                                <small></small>
                            </div>
                            <div class="my-5">
                                <input type="submit" class="btn btn-primary w-50 fs-5 fw-bold form-control" value="Change">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>    
        <%}%>
        <!-- User Change Password End -->


        <!-- User Wallet -->
        <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12" id="my-wallet">
            <div class="row mt-4">
                <div>
                    <h4 class="text-center fw-bold mt-2 bg-light">WALLET</h4>
                <div class="bg-light "><br>
                        <span class="fs-4  ms-5 fw-bold">Wallet Amount: &#8377; <span id="wallet-amount"></span> </span>
                    <div class="row mx-1 my-1 bg-light">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                            <button class="btn btn-primary btn-md m-4 w-75">Add Amount</button>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                            <button class="btn btn-primary btn-md m-4 w-75">View History</button>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                
                    <table class="table table-striped" style="background-color: white;">
                        <thead>
                            <th>S.No.</th>
                            <th>Date</th>
