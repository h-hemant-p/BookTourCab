                        <i class="fas fa-user-circle text-light display-2 m-4"></i><br>
                        <% if(user.name){ %>
                            <span class="text-light fw-bold"><%= user.name %></span><br>
                            <span class="text-light fw-bold"><%= user.contact_no %></span>
                        <% }else{ %>
                            <span class="text-light fw-bold"><%= user.email %></span><br>
                            <span class="text-light fw-bold"><%= user.contact_no %></span>
                        <% } %>
                    </div>
                    <br><br>
                    <div class="mx-5 fw-bold text-light">
                        <span class="dashboard-links" onclick="dashboardChoices('my-profile')" >My Profile</span><br><br>
                        <span class="dashboard-links" onclick="dashboardChoices('my-bookings')" >My Bookings</span><br><br>
                        <% if(user.is_driver){ %>
                            <span class="dashboard-links" onclick="dashboardChoices('my-driving-history')">My Driving History</span><br><br>
                        <% }%>
                        <% if(user.is_owner){ %>
                            <span class="dashboard-links" onclick="dashboardChoices('my-vehicles')">My Vehicles</span><br><br>
                            <span class="dashboard-links" onclick="dashboardChoices('my-drivers')">My Drivers</span><br><br>
                            <span class="dashboard-links" onclick="dashboardChoices('my-vehicles-history')">My Vehicle History</span><br><br>
                        <% } %>
                        <span class="dashboard-links" onclick="dashboardChoices('my-wallet')" >My Wallet</span><br><br>
                        <% if(!user.password){%>
                            <span class="dashboard-links" onclick="dashboardChoices('change-password')" >Create Password</span><br><br>
                        <%}else{%>
                            <span class="dashboard-links" onclick="dashboardChoices('change-password')" >Change Password</span><br><br>
                         <%}%>
                          
                         
                         <%if(!user.is_driver && user.is_complete){%>
                            <button class="btn btn-primary  mt-2 w-75 text-dark" data-bs-toggle="modal" data-bs-target="#driverRegistrationForm" style="background-color: #d8d6e3;" >Become Driver</button>
                        <%}else if(!user.is_driver && !user.is_complete){%>
