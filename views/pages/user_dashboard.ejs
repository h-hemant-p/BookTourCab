               <div class="row  ms-lg-1 mt-4 myDashboardCard " style="background-color: rgb(195, 194, 194);">
                   <h4 class="text-center fw-bold my-2 m-2 " style="background-color: white; width: 98%;">Create Password</h4>
                   <div class="m-2" style="background-color: white; width: 98%;">
                      <div align="center" style="background-color: white;">
                          <form action="/user/createpassword" method="post" onsubmit="return submitCreateFormData()">
                             <div class="my-5 form-field">
                                <label class="fw-bold fs-5 " for="newpassword">Enter Password*</label>
                                <input type="password" class="w-50 form-control" placeholder="Enter password" id="createnewpass" name="newpassword" onkeyup="createCheckNewPass4()">
                                <small></small>
                              </div>
                             <div class="my-5 form-field">
                                <label class="fw-bold fs-5" for="createconfirmpass">Confirm Password*</label>
                                <input type="password" class="w-50 form-control" placeholder="Enter confirm password" id="createconfirmpass" name="createconfirmpass" onkeyup="createCheckCnfPass4()">
                                <small></small>
                              </div>
                             <div class="mt-5">
                                <input type="submit" class="btn btn-primary w-50 fs-5 fw-bold form-control" value="Create">
                             </div><br><br>
                          </form>
                      </div>
                   </div>
               </div>
           </div>    
           <!-- User Create Password End -->
           <%}else{%>
