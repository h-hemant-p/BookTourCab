                            </tr>
                        </tbody>
                    </table>
                </div>
                </div>
            </div>
        </div>
        <!-- User Wallet -->
           
           
        <!-- Owner Vehicles List Start -->
        <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12" id="my-vehicles">
            <div class="table-responsive">
                <h4 class="text-center fw-bold mt-2 ">Vehicle List</h4><br>
                <table class="table" >
                    <thead>
                        <th>Registration Number</th>
                        <th>Vehicle Class</th>
                        <th>Fuel Type</th>
                        <th>Add/Update Insurance</th>
                        <th>Delete Vehicle</th>
                    </thead>
                    <tbody>
                        <%  if(ownerDetails.vehicles){
                            ownerDetails.vehicles.forEach((vehicle)=> { %>  
                                <tr>
                                <td id="vehichleReegistrationno"><%= vehicle.reg_number %></td>
                                <td><%= vehicle.vehicle_class %></td>
                                <td><%= vehicle.fuel_type %></td>
                                <% if(!vehicle.have_insurance){%>
                                    <td><button class="btn btn-success" data-bs-toggle="modal" data-bs-target="#vehicle_insurance" onclick="setRegistrationNumber('<%= vehicle.reg_number %>','<%=ownerDetails._id %>')">Insurance</button></td>
                                <%}else{%>
                                    <td><button  data-bs-toggle="modal" data-bs-target="#updateinsurance" class="open-modal btn btn-warning text-light" data-insurance_details="<%= vehicle.insurance_details %>">Update</button></td>
                                <%}%>
                                    <td><button  class=" btn btn-danger " data-insurance_details=""><a href="/user/deletevehicle?reg_number=<%=vehicle.reg_number %>" class="text-decoration-none text-light">Delete</a></button></td>
                                </tr>
                            <%});
                        }%>
                    </tbody>
                </table>
                <!-- Insurance Update Modal start  -->
                <div class="modal fade" id="updateinsurance" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl " role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLabel">Update Insurance Form</h5>
                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <div class="modal-body">
                            <form action="/user/updateinsurancedetails" method="post">
                                <div class="container ">
                                    <div class="border bg-primary">
                                    <h5 class="m-2 fw-bold text-center text-light">VEHICLE INSURANCE FORM</h5>
                                    </div>
                                </div>
                                <div class="container"><br>
                                    <div class="row">
                                        <div class="mb-3 col-lg-6">
                                            <input type="hidden" id="insuranceid" name="insuranceid" value="">
                                            <label for="policy_number" class=" form-label fw-bold">Policy Number </label><br>
                                            <input type="text" class="form-control border-dark w-75 " placeholder="Enter Policy Number" id="policynumber" name="policynumber" aria-describedby="ride">
                                            <span class="text-danger ms-4" id="policy-number-span"></span>
                                        </div>
                                        <div class="mb-3 col-lg-6">
                                            <label for="policy_holder_name" class=" form-label fw-bold">Policy Holder Name</label><br>
