<!-- User Change Password -->
<div class="col-lg-8 col-md-12 col-sm-12 col-xs-12" id="change-password">
    <div class="row  ms-lg-1 mt-4 myDashboardCard " style="background-color: rgb(195, 194, 194);">
        <h4 class="text-center fw-bold my-2 m-2 " style="background-color: white;">Change Password</h4>
        <div class="m-2" style="background-color: white;">
            
            <div class="text-center" style="background-color: white;">
                <div class="">
                    <label class=" fw-bold fs-5 mt-3" for="oldpassword">Old Password</label><br>
                    <input type="password" class="w-50 text-start border-dark " placeholder="Enter Old Password" id="oldpassword">
                </div>
                <div class="my-5">
                    <label class="fw-bold fs-5 " for="newpassword">New Password</label><br>
                    <input type="password" class="w-50 text-start border-dark " placeholder="Enter Old Password" id="newpassword">
                </div>
                <div class="my-5">
                    <label class="fw-bold fs-5  " for="confirmpassword">Confirm Password</label><br>
                    <input type="password" class="w-50 text-start border-dark " placeholder="Enter Old Password" id="confirmpassword">
                </div>
                <div class="mt-5">
                   <button class="btn btn-primary w-25 fs-5 fw-bold form-control"  onsubmit="return validatePassword()">Submit</button><br><br>
                </div>
                <br>
            </div>
           
        </div>
    </div>
</div>    
