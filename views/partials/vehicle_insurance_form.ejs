<!-- Modal Body -->
<!-- if you want to close by clicking outside the modal, delete the last endpoint:data-bs-backdrop and data-bs-keyboard -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/formValidation.css">
  <title>Document</title>
</head>
<body>
<!-- <button type="button" class="btn btn-primary btn-lg" >Vehicle Insurance</button> -->
  

<div class="modal fade" id="vehicle_insurance" tabindex="-1" data-bs-backdrop="static" data-bs-keyboard="false" role="dialog" aria-labelledby="modalTitleId" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl " role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modal-titleId">Vehicle Insurance</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <form action="/user/addInsurance" onsubmit="return submitInsuranceDetails()" method="post">
        <div class="container ">
          <div class="border bg-primary">
            <h5 class="m-2 fw-bold text-center text-light">VEHICLE INSURANCE FORM</h5>
          </div>
        </div>

        <div class="container"><br>
          <div class="row">
            <div class="mb-3 col-lg-6 form-field">
              <input type="hidden"  id="registrationNumber" name="registrationNumber">
              <!-- <input type="hidden" id="ownerId" name="ownerId" value=""> -->

              <label for="policy_number" class=" form-label fw-bold">Policy Number </label>
              <input type="text" class="form-control w-75 " placeholder="Enter Policy Number" id="policy-number" name="policynumber" onkeyup="checkPolicyNumber8()" aria-describedby="ride">
              <small></small>
            </div>
            <div class="mb-3 col-lg-6 form-field">
              <label for="policy_holder_name" class=" form-label fw-bold">Policy Holder Name</label>
              <input type="text" class="form-control w-75" placeholder="Enter Policy Holder Name" id="policy-holder-name" name="policyholdername" onkeyup="checkPolicyHolderName8()" aria-describedby="ride">
              <small></small>
            </div>
          </div>

          <div class="row">
            <div class="mb-3 col-lg-6 form-field">
              <label for="policy_holder_contact" class=" form-label fw-bold">Policy Holder Contact </label>
              <input type="text" class="form-control w-75" placeholder="Enter Policy Holder Contact" id="policy-holder-contact" name="policyholdercontact" onkeyup="checkPolicyHolderContact8()" aria-describedby="ride">
             <small></small>
            </div>
            <div class="mb-3 col-lg-6 form-field">
              <label for="policy_holder_aadhar" class=" form-label fw-bold ">Policy Holder Aadhar Number</label>
              <input type="text" class="form-control w-75" placeholder="Enter Aadhar Number" id="policy-holder-aadhar" name="policyholderaadhar" onkeyup="checkAadharNumber8()" aria-describedby="ride">
              <small></small>
            </div>
          </div>

          <div class=" row">
            <div class="mb-3 col-lg-6 form-field">
              <label for="coverage_start_date" class=" form-label fw-bold">Coverage Start Date</label>
              <input type="date" class="form-control  w-75" id="coverage-start-date" name="coveragestartdate" aria-describedby="ride" onchange="checkCoveageStartDate8()">
              <small></small>
            </div>
            <div class="mb-3 col-lg-6 form-field">
              <label for="coverage_end_date" class=" form-label fw-bold ">Coverage End Date</label>
              <input type="date" class="form-control  w-75 " id="coverage-end-date" name="coverageenddate" aria-describedby="ride" onchange="checkCoveageEndDate8()">
              <small></small>
            </div>
          </div>

          <div class=" row ">
            <div class="mb-3 col-lg-6 form-field">
              <label for="insurance_premium_amount" class=" form-label fw-bold">Insurance Premium Amount</label>
              <input type="text" class="form-control  w-75" placeholder="Enter Insurance Premium Amount" id="insurance-premium-amount" name="insurancepremiumamount" aria-describedby="ride" onkeyup="checkPremiumAmount8()">
              <small></small>
            </div>
            <div class="mb-3 col-lg-6 form-field">
              <label for="coverage_amount" class=" form-label fw-bold">Coverage Amount</label>
              <input type="text" class="form-control  w-75" placeholder="Enter Coverage Amount" id="coverage-amount" name="coverageamount" aria-describedby="ride" onkeyup="checkCoverageAmount8()">
              <small></small>
            </div>
          </div>

          <div class="row">
            <div class="mb-3 col-lg-6 form-field">
              <label for="insurance_provider" class=" form-label fw-bold ">Insurance Provider</label>
              <input type="text" class="form-control w-75 " placeholder="Enter Insurace Provider" id="insurance-provider" name="insuranceprovider" aria-describedby="ride" onkeyup="checkInsuranceProvider8()">
              <small></small>
            </div>
          </div>

        </div>
      </div>
      <div class="row mt-2">
        <div class="mb-3 col-lg-6 form-field">
          <input type="button" class="btn btn-secondary w-75 ms-2" value="cancel" data-bs-dismiss="modal">
        </div>
        <div class="mb-3 col-lg-6 form-field">
          <input type="submit" class="btn btn-primary w-75" value="Submit">
        </div>
      </div>
    </form>
    </div>
  </div>
</div>
<!-- Vehicle Insurance Modal End -->
</body>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
<script src="/js/vehicle_insurance_form.js"></script>
</html>